// selection sort by Kunal Khairnar 
```c
#include<stdio.h>

void selectionSort(int *arr,int size){
    int i;
    for(i=0;i<size-1;i++){
        for(int j=i+1;j<size;j++){
        if(arr[j]<arr[i]){
            int temp=arr[i];
            arr[i]=arr[j];
            arr[j]=temp;
        }
    }
}
}

int main()
{
    int size=5;
    int arr[size];
    printf("Enter an array");
    for(int i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
    }
    selectionSort(&arr,size);
    printf("Sorted array : \n");
    printf("[");
    for(int i =0;i<size;i++){
        printf("%d ",arr[i]);
    }
    printf("]");
    return 0;
}
```
